{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "target": "esnext",
    "module": "esnext",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
